class employee{
int id;
String name;
float salary;
void insert(int i,String n,float s)
{
id=1;
name=n;
salary=s;
}
void display( )
{
System.out.println(id+""+name+""+salary);
}
}
class totalemployee
{
public static void main(String [ ]args)
{
employee e1=new employee( );
employee e2=new employee( );
employee e3=new employee( );
e1.insert(100,"nandu",200000);
e2.insert(101,"bhavya",200000);
e3.insert(102,"nani",200000);
e1.display( );
e2.display( );
e3.display( );
}
}
